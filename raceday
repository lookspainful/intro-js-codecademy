let raceNumber = Math.floor(Math.random() * 1000);

let earlyRegister = true;
let runnerAge;

if (runnerAge >= 18 && earlyRegister === false)
{
  raceNumber += 1000;
  console.log(`Early registrants begin at 9:30am. Your number is ${raceNumber}.`);
}
else if (runnerAge >= 18 && earlyRegister === false)
{
  console.log(`Late adults run at 11:00am. Your race number is ${raceNumber}`);
}
else if (runnerAge < 18)
{
  console.log(`Youth registrants run at 12:30 pm (regardless of registration). Your race number is ${raceNumber}.`);
}
else
{
  console.log('An error has occurred. See the registration desk.');
}
